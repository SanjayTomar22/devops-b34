pipeline {
    agent any
    
    stages {
        
        stage ('Git-Pull') {
            steps {
                git branch: 'devops', url: 'https://github.com/shubhamkalsait/EasyCRUD.git'
            }
        }
         stage ("Build") {
            steps {
                sh '''
                cd backend
                mvn clean package -DskipTests'''
            }
        }
         stage ("Test") {
            steps {
                echo 'Test Success'
            }
        }
         stage ("Deploy") {
            steps {
                echo 'Deploy Success'
            }
        }
        
    }
    
}
