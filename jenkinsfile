pipeline {
    agent any
    
    stages {
        
        stage ('Git-Pull') {
            steps {
                echo 'Git-Pull Success'
            }
        }
         stage ("Build") {
            steps {
                echo 'Build Success'
            }
        }
         stage ("Test") {
            steps {
                echo 'Test Success'
            }
        }
         stage ("Deploy") {
            steps {
                echo 'Deploy Success'
            }
        }
        
    }
    
}
